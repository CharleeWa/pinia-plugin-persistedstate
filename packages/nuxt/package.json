  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/prazdevs/pinia-plugin-persistedstate/tree/main/packages/nuxt"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
