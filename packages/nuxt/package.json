  "devDependencies": {
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.1",
    "@pinia/nuxt": "^0.5.1",
    "eslint": "^9.2.0",
    "nuxt": "^3.11.2"
  }
